  "type": "module",
  "scripts": {
    "audit": "better-npm-audit audit",
    "start:dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "ts": "tsc --noEmit",
    "test": "echo \"No test specified\"",
    "cspell:lint": "cspell \"**/*.{ts,tsx}\"",
    "cspell:populate": "cspell --words-only --unique \"**/*.{ts,tsx}\" | sort --ignore-case >> cspell-words.txt",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "commitlinter": "commitlint -e $1",
    "stagelinter": "lint-staged"
  },
  "dependencies": {
    "@atproto/api": "^0.13.15",
    "@clerk/clerk-react": "^5.15.2",
    "@clerk/clerk-sdk-node": "^5.0.61",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@formspark/use-formspark": "^0.1.39",
    "@hookform/resolvers": "^3.9.1",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@netlify/functions": "^2.8.2",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@tippyjs/react": "^4.2.6",
    "better-npm-audit": "^3.7.3",
    "crypto-js": "^4.2.0",
    "easy-peasy": "^6.0.4",
    "jwt-decode": "^4.0.0",
    "loglevel": "^1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.2",
    "react-router-dom": "^6.28.0",
    "react-toastify": "^10.0.5",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@cspell/dict-de-de": "^4.0.0",
    "@eslint/js": "^9.14.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.2.24",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "cspell": "^8.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.4",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.11"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
