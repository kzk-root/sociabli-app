{
  "name": "sociabli-app",
  "private": true,
  "version": "0.9.0",
  "description": "Sociabli App",
  "author": "Mark Schmeiser<mark.schmeiser@konzentrik.de>",
  "contributors": [
    {
      "name": "Maurice Renck",
      "email": "maurice.renck@konzentik.de"
    }
  ],
  "type": "module",
  "scripts": {
    "audit": "better-npm-audit audit",
    "start:dev": "vite",
    "build": "npm run build:react && npm run build:11ty && npm run copy:blog && npm run copy:rss",
    "build:react": "tsc && vite build",
    "preview": "vite preview",
    "ts": "tsc --noEmit",
    "test": "echo \"No test specified\"",
    "cspell:lint": "cspell \"**/*.{ts,tsx}\"",
    "cspell:populate": "cspell --words-only --unique \"**/*.{ts,tsx}\" | sort --ignore-case >> cspell-words.txt",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "commitlinter": "commitlint -e $1",
    "stagelinter": "lint-staged",
    "build:11ty": "cd blog && npm install && npx @11ty/eleventy",
    "copy:blog": "cp -r blog/dist/blog dist/blog",
    "copy:rss": "cp -r blog/dist/feed.xml dist/blog/feed.xml"
  },
  "dependencies": {
    "@atproto/api": "^0.14.2",
    "@clerk/clerk-react": "^5.23.0",
    "@clerk/clerk-sdk-node": "^5.1.6",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@formspark/use-formspark": "^0.1.39",
    "@hookform/resolvers": "^4.1.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@netlify/functions": "^3.0.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@supabase/supabase-js": "^2.49.3",
    "@tippyjs/react": "^4.2.6",
    "better-npm-audit": "^3.7.3",
    "crypto-js": "^4.2.0",
    "easy-peasy": "^6.1.0",
    "jwt-decode": "^4.0.0",
    "loglevel": "^1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^7.2.0",
    "react-toastify": "^11.0.3",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@cspell/dict-de-de": "^4.0.0",
    "@eslint/js": "^9.20.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.2.24",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "cspell": "^8.17.4",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
